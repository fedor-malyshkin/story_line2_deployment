processManagement:
   fork: true
   pidFilePath: "<%= $storyline_infra::mongodb::pid_file -%>"
net:
#   bindIp: 127.0.0.1
   port: 27017
storage:
   dbPath: "<%= $storyline_infra::mongodb::dir_data -%>"
systemLog:
   destination: file
   path: "<%= $storyline_infra::mongodb::dir_logs -%>/mongod.log"
   logAppend: true
storage:
   journal:
      enabled: true
